#JSGF V1.0;

grammar digits;

<digit> = one | two | three | four | five | six | seven | eight | nine;
<dozens> = twenty | thirty | forty | fifty | sixty | seventy | eighty | ninety;
<others> = zero | eleven | twelve | thirteen | fourteen | sixteen | seventeen | eighteen | nineteen;

public <digits> = <digit> | <dozens> <digit> | <others>;
